[user]
	name = Raymond Tang
	email = Raytray25@gmail.com
[credential]
	helper = cache
[core]
	excludesfile = /home/raytray/.gitignore_global
[alias]
	ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
	ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
	ld = log --pretty=format:"%C(yellow)%h\\ %ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=relative
	la = "!git config -l | grep alias | cut -c 7-"
	cv = commit --verbose
[merge]
    tool = mymeld
[mergetool "mymeld"]
    cmd = /home/raytray/Dropbox/Configs/mergetool.sh $BASE $LOCAL $REMOTE $MERGED
[github]
	user = Raytray
[gist]
	private = no
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
       	whitespace = red reverse
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
